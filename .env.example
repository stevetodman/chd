# Frontend (Vite) configuration
# These values are required to connect the UI to your Supabase project.
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=
VITE_APP_NAME=CHD QBank

# Backend automation helpers
# Never commit secretsâ€”load service-role credentials via your shell or a secrets manager.
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# Invite seeding placeholders (set just-in-time when running scripts)
INVITE_CODE=<YOUR_INVITE_CODE>
INVITE_EXPIRES=<YYYY-MM-DD>
INVITE_CODE_SALT=

# Seed admin overrides for `npm run seed:full` (optional)
SEED_ADMIN_EMAIL=admin@example.com
SEED_ADMIN_PASSWORD=Admin123!
SEED_ADMIN_ALIAS=demo-admin

# Local infrastructure used by docker-compose
POSTGRES_DB=chd_qbank
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# Analytics verification tuning (optional)
HEATMAP_VERIFY_RETRY_BASE_MS=250
HEATMAP_VERIFY_CONCURRENCY=1
HEATMAP_VERIFY_BATCH=500
HEATMAP_VERIFY_QUESTIONS=200
HEATMAP_VERIFY_USERS=50
HEATMAP_VERIFY_NON_PROD_HOSTS=
HEATMAP_VERIFY_PROD_HOSTS=
HEATMAP_VERIFY_ALLOW_PROD=false
