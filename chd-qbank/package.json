{
  "name": "chd-qbank",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:staging": "vite --mode staging",
    "dev:production": "vite --mode production",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "preview": "vite preview",
    "preview:staging": "vite preview --mode staging",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:auth": "eslint --ext .ts,.tsx src/pages/Login.tsx src/pages/Signup.tsx src/locales",
    "typecheck": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "build:scripts": "tsc -p tsconfig.scripts.json",
    "notice": "node scripts/generate-notice.mjs",
    "check:migration-safety": "npm run build:scripts && node scripts/dist/check-migration-safety.js",
    "seed:invite": "node scripts/seed-invite.mjs",
    "seed:full": "npm run build:scripts && node scripts/dist/seed/full.js",
    "verify:seed": "npm run build:scripts && node scripts/dist/verifySeed.js",
    "verify:analytics:heatmap": "node scripts/verify-analytics-heatmap.mjs",
    "verify:policies": "node scripts/verify-policies.mjs",
    "verify:storage": "node scripts/verify-storage-buckets.mjs",
    "migrate:qbank": "npm run build:scripts && node scripts/dist/migrate-qbank.js",
    "migrate:qbank:dry": "npm run build:scripts && DRY_RUN=1 node scripts/dist/migrate-qbank.js",
    "blueprint:coverage": "npm run build:scripts && node scripts/dist/blueprint-coverage.js",
    "export:anki": "node scripts/export-anki.mjs"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/supabase-js": "^2.44.3",
    "chart.js": "^4.4.1",
    "classnames": "^2.5.1",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.22.1",
    "rehype-highlight": "^6.0.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^3.0.1",
    "workbox-cacheable-response": "^7.1.0",
    "workbox-expiration": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@axe-core/react": "file:vendor/@axe-core/react",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.9.2",
    "@types/papaparse": "^5.3.7",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.16",
    "axe-core": "file:vendor/axe-core",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "file:eslint-plugin-jsx-a11y",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "fast-glob": "^3.3.2",
    "jest-axe": "file:vendor/jest-axe",
    "jsdom": "^27.0.1",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.3.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.4"
  }
}
